import random
L=[]
for i in range(5):
    a=random.randrange(1,100,2)
    L.append(a)
print ("list of odd numbers ",L)    
  
L1=[]
for i in range (5):
    b=random.randrange(2,100,2)
    L1.append(b)

print( "list of even numbers ",L1)

L.pop(2) 
L.insert(2,L1)
print (  "after inserting even list at third posotion ",L)

print ("after flattering")

flattened_list=[]
for item in L:
    if isinstance (item,list):
        flattened_list.extend(item)
    else:

        flattened_list.append(item)


print (flattened_list)

        
